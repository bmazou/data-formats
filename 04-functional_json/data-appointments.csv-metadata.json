{
    "@context": [
        "http://www.w3.org/ns/csvw",
        {
            "@language": "en"
        }
    ],
    "@type": "Table",
    "@id": "https://example.org/data-appointments.csv-metadata.json",
    "url": "data-appointments.csv",
    "tableSchema": {
